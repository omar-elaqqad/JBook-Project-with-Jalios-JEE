<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="generated.DBComment" table="G_DBCOMMENT">
    <cache usage="read-write" />  
    <id name="rowId" column="rowId">
      <generator class="native"/>
    </id>
    <property name="cdate" not-null="true" index="IG_DBCOMMENT_CDATE" />
    <property name="mdate" not-null="true" index="IG_DBCOMMENT_MDATE" />
    <property name="authorId" length="32" index="IG_DBCOMMENT_AUTHORID" />
    
    <property name="pstatus" not-null="true" index="IG_DBCOMMENT_PSTATUS" />
    <property name="workspaceId" not-null="true" length="32" index="IG_DBCOMMENT_WSID" />
    
    <property name="pdate"  index="IG_DBCOMMENT_PDATE" />
    <property name="edate" />
    <property name="adate" />
    <property name="title" />
    <property name="workflowId" index="IG_DBCOMMENT_WFID" />
    <property name="udate" />
    <property name="majorVersion" />
    <property name="minorVersion" />
    <property name="voteCount" not-null="false" index="IG_DBCOMMENT_VOTECOUNT" />
    <property name="voteScore" not-null="false" index="IG_DBCOMMENT_VOTESCORE" />
    <property name="opAuthorId" length="32" index="IG_DBCOMMENT_OPAUTHORID"  />
    <property name="readAck" />
    
    <property name="name" />
    
    
    <property name="email" />
    
    
    <property name="webSite" />
    
    
    <property name="description" type="text" length="65536" />
    
    
    <property name="targetContentId" length="64" index="IG_DBCOMMENT_TARGETCONTENT" />
    
    
    <property name="ipAddress" />
    
    
    <property name="userAgent" />
    
    
    <property name="referer" />
    
    
    <property name="privateComment" />
    
    
  </class>
</hibernate-mapping>
