<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="generated.BlogPost" table="G_BLOGPOST">
    <cache usage="read-write" />  
    <id name="rowId" column="rowId">
      <generator class="native"/>
    </id>
    <property name="cdate" not-null="true" index="IG_BLOGPOST_CDATE" />
    <property name="mdate" not-null="true" index="IG_BLOGPOST_MDATE" />
    <property name="authorId" length="32" index="IG_BLOGPOST_AUTHORID" />
    
    <property name="pstatus" not-null="true" index="IG_BLOGPOST_PSTATUS" />
    <property name="workspaceId" not-null="true" length="32" index="IG_BLOGPOST_WSID" />
    
    <property name="pdate"  index="IG_BLOGPOST_PDATE" />
    <property name="edate" />
    <property name="adate" />
    <property name="title" />
    <property name="workflowId" index="IG_BLOGPOST_WFID" />
    <property name="udate" />
    <property name="majorVersion" />
    <property name="minorVersion" />
    <property name="hasNoReadRights" index="IG_BLOGPOST_HASNOREADRIGHTS" />
    <property name="tracked" />
    <property name="readerCount" index="IG_BLOGPOST_READERCOUNT" />
    <property name="voteCount" not-null="false" index="IG_BLOGPOST_VOTECOUNT" />
    <property name="voteScore" not-null="false" index="IG_BLOGPOST_VOTESCORE" />
    
    <property name="mainInstanceId" not-null="false" length="32" index="IG_BLOGPOST_MAINSTID" />    
    <property name="mergeId" length="32" />    
    <property name="mergeDate" />
    <property name="hasAttachWS" index="IG_BLOGPOST_HATHWS"  />
    <property name="opAuthorId" length="32" index="IG_BLOGPOST_OPAUTHORID"  />
    <property name="readAck" />
    <property name="restrictUpdateRights" />
    
    <property name="roleMapStr" type="text" length="65536" />
    <property name="titleMLE" type="text" length="1048576" />
    <property name="mainLanguage" />
    <property name="content" type="text" length="65536" />
    
    
    <property name="contentMLE" type="text" length="1048576" />
    <property name="summary" type="text" length="65536" />
    
    
    <property name="summaryMLE" type="text" length="1048576" />
    <property name="blogId" length="64" index="IG_BLOGPOST_BLOG" />
    
    
    <property name="featuredImage" />
    
    
    <property name="featuredImageAlt" />
    
    
    <property name="featuredImageAltMLE" type="text" length="1048576" />
    <property name="thumbnailImage" />
    
    
    <property name="thumbnailImageAlt" />
    
    
    <property name="thumbnailImageAltMLE" type="text" length="1048576" />
    
    <!--  COLLECTIONS FOR CATEGORIES -->
    <set name="catIdSet" table="G_BLOGPOST_CATSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_CATSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_CATSET"/>
      </element>      
    </set>
    
    <set name="allCatIdSet" table="G_BLOGPOST_ALLCATSET"  lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_ALLCATSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_ALLCATSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR READ-RIGHTS -->
    <set name="authGrpIdSet" table="G_BLOGPOST_AUTHGRPSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_AUTHGRPSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_AUTHGRPSET"/>
      </element>      
    </set>    
    
    <set name="authMbrIdSet" table="G_BLOGPOST_AUTHMBRSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_AUTHMBRSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_AUTHMBRSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR UPDATE-RIGHTS -->
    <set name="updateGrpIdSet" table="G_BLOGPOST_UPDATEGRPSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_UPDATEGRPSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_UPDATEGRPSET"/>
      </element>      
    </set>    
    
    <set name="updateMbrIdSet" table="G_BLOGPOST_UPDATEMBRSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_UPDATEMBRSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_BLOGPOST_UPDATEMBRSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR FRIENDLYURL -->
    <set name="dbFriendlyURLSet" table="G_BLOGPOST_FURLSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_FURLSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
    
    <!--  COLLECTIONS FOR attachWSIdSet -->
    <set name="attachWSIdSet" table="G_BLOGPOST_AWSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_BLOGPOST_AWSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
  </class>
</hibernate-mapping>
