<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="generated.MemberProfile" table="G_MEMBERPROFILE">
    <cache usage="read-write" />  
    <id name="rowId" column="rowId">
      <generator class="native"/>
    </id>
    <property name="cdate" not-null="true" index="IG_MEMBERPROFILE_CDATE" />
    <property name="mdate" not-null="true" index="IG_MEMBERPROFILE_MDATE" />
    <property name="authorId" length="32" index="IG_MEMBERPROFILE_AUTHORID" />
    
    <property name="pstatus" not-null="true" index="IG_MEMBERPROFILE_PSTATUS" />
    <property name="workspaceId" not-null="true" length="32" index="IG_MEMBERPROFILE_WSID" />
    
    <property name="pdate"  index="IG_MEMBERPROFILE_PDATE" />
    <property name="edate" />
    <property name="adate" />
    <property name="title" />
    <property name="workflowId" index="IG_MEMBERPROFILE_WFID" />
    <property name="voteCount" not-null="false" index="IG_MEMBERPROFILE_VOTECOUNT" />
    <property name="voteScore" not-null="false" index="IG_MEMBERPROFILE_VOTESCORE" />
    <property name="opAuthorId" length="32" index="IG_MEMBERPROFILE_OPAUTHORID"  />
    <property name="readAck" />
    
    <property name="introduction" type="text" length="65536" />
    
    
    <list name="interest" table="G_MEMBERPROFILE_INTEREST">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEMBERPROFILE_INTEREST_I"/>
      </key>      
      <list-index column="POSITION" />
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </list>
    <property name="dateOfBirth" />
    
    
    <property name="dateOfArrival" />
    
    
  </class>
</hibernate-mapping>
