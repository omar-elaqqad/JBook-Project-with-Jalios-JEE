<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="generated.Media" table="G_MEDIA">
    <cache usage="read-write" />  
    <id name="rowId" column="rowId">
      <generator class="native"/>
    </id>
    <property name="cdate" not-null="true" index="IG_MEDIA_CDATE" />
    <property name="mdate" not-null="true" index="IG_MEDIA_MDATE" />
    <property name="authorId" length="32" index="IG_MEDIA_AUTHORID" />
    
    <property name="pstatus" not-null="true" index="IG_MEDIA_PSTATUS" />
    <property name="workspaceId" not-null="true" length="32" index="IG_MEDIA_WSID" />
    
    <property name="pdate"  index="IG_MEDIA_PDATE" />
    <property name="edate" />
    <property name="adate" />
    <property name="title" />
    <property name="workflowId" index="IG_MEDIA_WFID" />
    <property name="udate" />
    <property name="majorVersion" />
    <property name="minorVersion" />
    <property name="hasNoReadRights" index="IG_MEDIA_HASNOREADRIGHTS" />
    <property name="tracked" />
    <property name="readerCount" index="IG_MEDIA_READERCOUNT" />
    <property name="voteCount" not-null="false" index="IG_MEDIA_VOTECOUNT" />
    <property name="voteScore" not-null="false" index="IG_MEDIA_VOTESCORE" />
    
    <property name="mainInstanceId" not-null="false" length="32" index="IG_MEDIA_MAINSTID" />    
    <property name="mergeId" length="32" />    
    <property name="mergeDate" />
    <property name="hasAttachWS" index="IG_MEDIA_HATHWS"  />
    <property name="opAuthorId" length="32" index="IG_MEDIA_OPAUTHORID"  />
    <property name="readAck" />
    <property name="restrictUpdateRights" />
    <property name="filename" index="IG_MEDIA_FILENAME" />
    <property name="originalFilename" />
    <property name="contentType"  index="IG_MEDIA_CONTENTTYPE"  />
    <property name="remoteUri" />
    <property name="uploadDate" />
    <property name="pdfUploadDate" />
    
    <property name="roleMapStr" type="text" length="65536" />
    <property name="titleMLE" type="text" length="1048576" />
    <property name="mainLanguage" />
    <property name="description" type="text" length="65536" />
    
    
    <property name="descriptionMLE" type="text" length="1048576" />
    <property name="mediaWidth" />
    
    
    <property name="mediaHeight" />
    
    
    <property name="mediaDuration" />
    
    
    
    <!--  COLLECTIONS FOR CATEGORIES -->
    <set name="catIdSet" table="G_MEDIA_CATSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_CATSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_CATSET"/>
      </element>      
    </set>
    
    <set name="allCatIdSet" table="G_MEDIA_ALLCATSET"  lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_ALLCATSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_ALLCATSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR READ-RIGHTS -->
    <set name="authGrpIdSet" table="G_MEDIA_AUTHGRPSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_AUTHGRPSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_AUTHGRPSET"/>
      </element>      
    </set>    
    
    <set name="authMbrIdSet" table="G_MEDIA_AUTHMBRSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_AUTHMBRSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_AUTHMBRSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR UPDATE-RIGHTS -->
    <set name="updateGrpIdSet" table="G_MEDIA_UPDATEGRPSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_UPDATEGRPSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_UPDATEGRPSET"/>
      </element>      
    </set>    
    
    <set name="updateMbrIdSet" table="G_MEDIA_UPDATEMBRSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_UPDATEMBRSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_MEDIA_UPDATEMBRSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR FRIENDLYURL -->
    <set name="dbFriendlyURLSet" table="G_MEDIA_FURLSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_FURLSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
    
    <!--  COLLECTIONS FOR attachWSIdSet -->
    <set name="attachWSIdSet" table="G_MEDIA_AWSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_MEDIA_AWSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
  </class>
</hibernate-mapping>
