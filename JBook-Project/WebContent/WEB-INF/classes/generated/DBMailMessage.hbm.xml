    <!--  COLLECTIONS FOR UPDATE-RIGHTS -->
    <set name="updateGrpIdSet" table="G_DBMAILMESSAGE_UPDATEGRPSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_DBMAILMESSAGE_UPDATEGRPSET_"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_DBMAILMESSAGE_UPDATEGRPSET"/>
      </element>      
    </set>    
    
    <set name="updateMbrIdSet" table="G_DBMAILMESSAGE_UPDATEMBRSET" lazy="true">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_DBMAILMESSAGE_UPDATEMBRSET_"/>
      </key>
      <element type="string">
        <column name="VALUE" length="64" index="IG_DBMAILMESSAGE_UPDATEMBRSET"/>
      </element>      
    </set>
    
    <!--  COLLECTIONS FOR FRIENDLYURL -->
    <set name="dbFriendlyURLSet" table="G_DBMAILMESSAGE_FURLSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_DBMAILMESSAGE_FURLSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
    
    <!--  COLLECTIONS FOR attachWSIdSet -->
    <set name="attachWSIdSet" table="G_DBMAILMESSAGE_AWSET">
      <cache usage="read-write" />
      <key>
        <column name="ITEM_ID" index="IG_DBMAILMESSAGE_AWSET_I"/>
      </key>
      <element type="string">
        <column name="VALUE"/>
      </element>      
    </set>    
  </class>
</hibernate-mapping>
