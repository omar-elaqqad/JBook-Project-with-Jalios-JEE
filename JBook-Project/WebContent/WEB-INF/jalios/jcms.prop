channel.reverse-proxy.conf2.base-url: 
channel.reverse-proxy.conf2.ssl-base-url: 

# ----------------------------------------------------------------------
# Options related to members
# ----------------------------------------------------------------------

# Session Tracker
#  n < 0 : Save all trace (warning: can really be memory consumming).
#  n = 0 : Do not save any trace.
#  n > 0 : Save n trace.
session-tracker.max-trace-nbr: 0

# Member : maximum number of last login date to store (must be > 0)
member.max-last-login-date: 10

# Member access control, introduce with JCMS-4784
member.read-right.enabled: false

# Member deletion behavior
# - true to enable a asynchronous delete, false to use legacy synchronous behavior
member.async-delete.enabled: true
# - timeout to use when waiting for asynchronous delete in legacy methods, 0 to wait indefinitely, value in milliseconds
member.async-delete.timeout: 0
# - timeout to use when waiting for asynchronous delete in user interface, value in milliseconds
member.async-delete.ui-timeout: 5000

# Security settings
# 
# Member password constraints
# Validation regex - since JCMS-3296
#   member.password-validation-regex:
# Length - since JCMS-7356
#   member.password-min-length:
#   member.password-max-length:
# Default minimum length (cf JCMS-7199)
member.password-min-length: 9
# Default maximum length to 70 for compatibility with BCrypt limit (cf JCMS-7205)
member.password-max-length: 70

# Allow workspace administrator (or ws admin through ACL) to add a member in a ws group descendant of a global group (since JCMS-3035)
member.rights.allow-global-group-wsadmin: true
# Allow member to modify their own login (since JCMS-7756)
member.rights.allow-login-change: true
# Allow member to modify their own email (since JCMS-7776)
member.rights.allow-mail-change: true

# Member CSV Import : 
member.csv-import.enabled: true
member.csv-import.max-member-per-import.number: 1000
member.csv-import.email-batch.deferr-seconds: 60
member.csv-import.max-error-to-show.number: 100
# Member CSV Import ajax request timeout (in millis, default 10 minutes)
# - Increase to allow longer import ajax requests (Adapt with application server timeout)
member.csv-import.ajax-timeout: 1200000
$id.member.csv-import.ws-root-cat: j_3


# Member edition 
# Show legacy address field ?
member.edit.show-address: true
# Automatically convert legacy addresses into new fields (street, postal code, pobox, locality, region, ...)
# *client side feature when member is edited* 
# this feature depends on Google Maps (using javascript specified in property 'google-js.maps') 
js.member.edit.auto-convert-address: true

# ----------------------------------------------------------------------

# Mail validation
mail.validator.class: com.jalios.util.mail.DefaultEmailValidator
workspace.incoming-mail.max-default-local-part-length: 60

# SMTP Server
mail.enabled:   false 
mail.smtp.host: 
mail.send-with-site-email: true

# Bug report recipient
bug-report.email: jcms-report@jalios.com

# Cookie time validity (in seconds)
# 1 day: 86400
# 1 month: 2592000
# 1 year: 31536000
cookie.max-age: 31536000
# Transient cookie max age (in seconds) - since JCMS-7586
# this value specifies the duration before transient cookie get removed from the DB,
# (it does not change the value of the HTTP cookie sent to the browser, which stays valid for the duration of the browser's life, hopefully less than this value)
# 2 weeks: 1209600
cookie.transient-max-age: 1209600
# Purge expired cookie from database, every day at 4
# Note : Purging cookie is not required for proper validation of expiration date, it simply clean old cookie no longer used
cookie.purge.schedule: 00 04 * * * * 

list.pager.max: 10
list.boolean.on: <img src='images/jalios/icons/bullet.gif' alt='' />
list.boolean.off: -

imgbrowser.default-view-mode:	thumb

display.navigate-root:
$id.favorite-ws-root:
display.portal.behavior: div
display.portal.behavior.backcompatibility: true
display.use-front-session-portal: true
display.prioritize-redirect: true

# CSV Export 
csv.charset: ISO-8859-1
# If exported field value starts with a formula character, prepend it with a ' (single apostrophe)
# This will prevents Excel and other spreadsheets editor from executing the formula
csv.cleanup.regex: ^\\s*[=+\\-@].*$
csv.cleanup.replacement: \\'$0

# ----------------------------------------------------------------------
# External URL
# ----------------------------------------------------------------------

url.jx:             http://community.jalios.com/
url.jx.plugins:     http://community.jalios.com/plugin/
url.jx.plugins.rss: http://community.jalios.com/plugin/rss/
url.jx.news.rss:    http://community.jalios.com/news/rss/

# ----------------------------------------------------------------------
# Authentication Manager
# ----------------------------------------------------------------------

# Allow Adminstrators to use persistent cookie
auth-mgr.allow-admin-cookie: true
auth-mgr.persistent.show-option: true
auth-mgr.persistent.default-value: false
auth-mgr.cookie-with-domain: false
# Parameter name used by login form and authentication handlers
auth-mgr.login-param.login:      JCMS_login
auth-mgr.login-param.password:   JCMS_password
auth-mgr.login-param.persistent: JCMS_persistentCookie
auth-mgr.login-param.op-login:   JCMS_opLogin
# Boolean property key used to authorized or forbid LDAP user to login with their local password (since JCMS-6504) 
auth-mgr.ldap-user.local-password-authorized: true

# Authentication throttle
#  Enable throttling or not ?
auth-throttle-auth-mgr.throttle-enabled: true
#  Time range in seconds during which the threshold is verified.
auth-throttle-auth-mgr.range-seconds: 3
#  Number of failed login attempts permitted in the specified time range.
auth-throttle-auth-mgr.threshold: 2
#  Number of seconds before the blocked/throttled can be released.
auth-throttle-auth-mgr.throttling-duration-seconds: 30

