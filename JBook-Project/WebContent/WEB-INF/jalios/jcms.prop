media.template.default.URL.vimeo.params.h: [0-9a-zA-Z]*
media.template.default.URL.vimeo.params.autopause: (true|false|0|1)
media.template.default.URL.vimeo.params.autoplay: (true|false|0|1)
media.template.default.URL.vimeo.params.background: (true|false|0|1)
media.template.default.URL.vimeo.params.byline: (true|false|0|1)
media.template.default.URL.vimeo.params.color: [0-9a-zA-Z]*
media.template.default.URL.vimeo.params.controls: (true|false|0|1)
media.template.default.URL.vimeo.params.dnt: (true|false|0|1)
media.template.default.URL.vimeo.params.keyboard: (true|false|0|1)
media.template.default.URL.vimeo.params.loop: (true|false|0|1)
media.template.default.URL.vimeo.params.muted: (true|false|0|1)
media.template.default.URL.vimeo.params.pip: (true|false|0|1)
media.template.default.URL.vimeo.params.playsinline: (true|false|0|1)
media.template.default.URL.vimeo.params.portrait: (true|false|0|1)
media.template.default.URL.vimeo.params.quality: (auto|240p|360p|540p|720p|1080p|2k|4k)
media.template.default.URL.vimeo.params.speed: (true|false|0|1)
media.template.default.URL.vimeo.params.texttrack: [a-zA-Z\.-]
media.template.default.URL.vimeo.params.title: (true|false|0|1)
media.template.default.URL.vimeo.params.transparent: (true|false|0|1)

# YouTube
media.template.default.URL.youtube.enabled:   true
media.template.default.URL.youtube.regex:     ^(https?:)?//(((www\\.)?youtube(-nocookie)?\\.com)|(youtu\\.be))/.*
media.template.default.URL.youtube.jsp:       /jcore/media/url/mediaTemplateYoutube.jsp
media.template.default.URL.youtube.yid-regex: .*(?:(?:youtu.be/)|(?:/embed/)|(?:/shorts/)|(?:/v/)|(?:[\\?&]v=))([a-zA-Z0-9_-]{6,11}).*
