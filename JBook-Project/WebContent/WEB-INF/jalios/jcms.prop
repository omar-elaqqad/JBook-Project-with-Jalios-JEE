# HTTP headers (applied to JSP only)
channel.http-header-excluded:      csspacker.jsp jspacker.jsp jalios-properties-js.jsp jalios-i18n-js.jsp tiny_mce_pack.jsp
channel.http-header.Cache-Control: no-cache, no-store, must-revalidate
channel.http-header.Pragma:        no-cache
channel.http-header.Expires:       0

# HTTP headers (applied to ALL JPlatform request), since JCMS-6392
#channel.global-http-header-excluded:
# X-Frame-Options, cf JCMS-5071
#channel.global-http-header.X-Frame-Options: sameorigin
# X-Content-Type-Options, cf JCMS-5509
channel.global-http-header.X-Content-Type-Options: nosniff
# X-XSS-Protection, cf JCMS-6393
channel.global-http-header.X-XSS-Protection: 1
# Referrer-Policy, cf JCMS-6749
channel.global-http-header.Referrer-Policy: strict-origin-when-cross-origin
# Feature-Policy, cf JCMS-7385
channel.global-http-header.Feature-Policy: accelerometer 'none'; autoplay 'none'; camera *; geolocation 'none'; gyroscope 'none'; magnetometer 'none'; microphone *; midi 'none'; payment 'none'; usb 'none';
# Permissions-Policy, cf JCMS-8551
channel.global-http-header.Permissions-Policy: accelerometer=(), autoplay=(), camera=*, geolocation=(), gyroscope=(), magnetometer=(), microphone=*, midi=(), payment=(), usb=()

