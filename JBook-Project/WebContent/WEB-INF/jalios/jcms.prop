wysiwyg.sanitize-html.attributes.id.whitelist:
wysiwyg.sanitize-html.attributes.id.whitelist-regex: 
wysiwyg.sanitize-html.attributes.id.blacklist: top topbar editForm sidebar AjaxCtxtDeflate CSRFTokenElm jalios-modal
wysiwyg.sanitize-html.attributes.id.blacklist-regex: ^((ctx|widget-|Portlet|modal|menu|topbar-).*)|(.*(-sidebar-).*)|(.*[fF]orm)$
wysiwyg.sanitize-html.attributes.id.default-behavior: true

# "class" attribute
# - Always explicitely authorize "wysiwyg" value, 
# - Blacklist some restricted internal class
# - Others unknown class names are accepted to provide backward compatibility
wysiwyg.sanitize-html.attributes.class.enabled: true
wysiwyg.sanitize-html.attributes.class.split-regex: \\s
wysiwyg.sanitize-html.attributes.class.whitelist: wysiwyg
wysiwyg.sanitize-html.attributes.class.whitelist-regex: 
wysiwyg.sanitize-html.attributes.class.blacklist:
wysiwyg.sanitize-html.attributes.class.blacklist-regex: ^(.*(-sidebar))|((ajax-|app|box|btn|card|ctxMenu|ctx|control|dropdown|edit|form|jalios-|loader|loading|input|modal|nav|panel|selection-|topbar-|tt-card|widget|sidebar-).*)$
wysiwyg.sanitize-html.attributes.class.default-behavior: true

# "style" attribute
# Authorized inline CSS properties
# Use a *whitelist* approach to prevent unwanted styling : 
# - Any CSS property (name) not part of the whitelist below is remove
wysiwyg.sanitize-html.inline-styles.default.enabled: true
wysiwyg.sanitize-html.inline-styles.default.whitelist: background background-color border border-collapse border-color border-radius border-spacing border-style border-width border-bottom border-bottom-color border-bottom-left-radius border-bottom-right-radius border-bottom-style border-bottom-width border-left border-left-color border-left-style border-left-width border-right border-right-color border-right-style border-right-width border-top border-top-color border-top-left-radius border-top-right-radius border-top-style border-top-width clear color float font-decoration font-size font-style font-weight font-family line-height height width max-height max-width min-height min-width margin margin-bottom margin-left margin-right margin-top padding padding-bottom padding-left padding-right padding-top text-align text-decoration text-indent text-justify vertical-align list-style-type 
wysiwyg.sanitize-html.inline-styles.default.default-behavior: false

# "role" attribute
# - Only authorize "heading" value, 
wysiwyg.sanitize-html.attributes.role.enabled: true
wysiwyg.sanitize-html.attributes.role.whitelist: heading


# Rewrite object>embed tag in HTML wysiwyg content
# with content generated by the MediaTag and its template API.  
# This process occurs at rendering time, 
#  - through WysiwygRenderer API invoked by <jalios:wysiwyg> tag.
#   cf http://issues.jalios.com/browse/JCMS-3680
#  - through WikiRenderer API for [media path=''] tag
wysiwyg.medias-rewrite.enabled: true
wysiwyg.medias-rewrite.use-dispatcher: true
wysiwyg.medias-rewrite.use-iframe: true

# Hard coded width and heigth version
wysiwyg.medias-rewrite.iframe-template: \
 <iframe class="embed-responsive-item" \
         width="{maxWidth}" height="{maxHeight}" \
         src="jcore/media/embed.jsp?src={mediaSource}&amp;width={maxWidth}&amp;height={maxHeight}" \
         allowfullscreen frameborder="0"></iframe>
     
# loading="lazy" attribute will be added to images from jalios:media     
wysiwyg.medias.lazy-load-images: true

# Bootstrap responsive version
wysiwyg.medias-rewrite.iframe-template: \
<div class="embed-responsive embed-responsive-16by9">\
 <iframe class="embed-responsive-item" \
         src="jcore/media/embed.jsp?src={mediaSource}&amp;width={maxWidth}&amp;height={maxHeight}" \
         allowfullscreen></iframe>\
</div>

# JS resized version
wysiwyg.medias-rewrite.iframe-template: \
 <iframe style="width:100%; overflow:hidden" \
         src="jcore/media/embed.jsp?src={mediaSource}&amp;width={maxWidth}&amp;height={maxHeight}&amp;autoresize=true" \
         allowfullscreen frameborder="0"></iframe>


# Wysiwyg Ajax include
wysiwyg.include.enabled: true

wysiwyg.include.gallery.enabled: true
wysiwyg.include.gallery.jsp: jcore/wysiwyg/wysiwygGallery.jsp

wysiwyg.include.query.enabled: true
wysiwyg.include.query.jsp: jcore/wysiwyg/wysiwygQuery.jsp

