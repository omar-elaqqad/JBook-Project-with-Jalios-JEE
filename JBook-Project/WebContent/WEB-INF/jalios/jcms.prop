# -----------------------------------------------------------------
# FileProcessor
# -----------------------------------------------------------------
# For all path of the FileProcessor configuration the following alias are available : 
# - at the begining : <realpath>, <datapath>, <webinfpath>, <uploadpath>, <jcmsuploadindexpath>
# - everywhere : <repositoryid>
# Whatever the OS or the FS, use '/' as file separator (as in an URI)

# ------- Configuration for JCMSUpload repository ------------

fileprocessor.repository.JCMSUpload.name: JCMS upload file repository
fileprocessor.repository.JCMSUpload.baseDirectory: <uploadpath>

# -- Scanning Properties -- 

# Optionnal. Name of the AlarmManager (and name of the thread)
# You may use alias : <repositoryid> and <repositoryname>
# fileprocessor.repository.JCMSUpload.alarmMgrName:

# RegExp to exclude some directory from file processing
fileprocessor.repository.JCMSUpload.excludedDirRegExp: (^CVS$)|(^\.svn$)|(^logs$)

# Repository scanning rythm can be defined either with schedulePeriod or with scheduleCron.
# Because the FileProcessor supports StoreListener invocation for the JCMSUpload 
# repository, the scanning is completely disabled for this repository. This will prevent
# a scanning of many many files to block the processing queue for incomming files.
#  
# 1. Schedule Period : duration in minutes between each scan of repository
fileprocessor.repository.JCMSUpload.schedulePeriod: 
# 2. Schedule Cron : 
#   for syntax usage details see http://community.jalios.com/jcms/jx_63894/jdring
fileprocessor.repository.JCMSUpload.scheduleCron:

# Maximum number of files per scan
fileprocessor.repository.JCMSUpload.maxFilesPerScan: 5000

# -- Processing Properties --

# If the duration of a processing is more than the processingLimitDurationString (in seconds)
# and an unknown exception is thrown, then the file is blacklisted for this processor
fileprocessor.repository.JCMSUpload.processingLimitDuration: 120

# maximum number of attempts allowed per processing before file 
# gets blacklisted for the corresponding component
fileprocessor.repository.JCMSUpload.processingMaximumAttempts: 5

# -- Indexing/Parsing Properties --
fileprocessor.repository.JCMSUpload.indexerClass: com.jalios.jcms.fileprocessor.repository.jcmsupload.JCMSUploadIndexer

# -- Postponed item check Properties --

# Optionnal. Name of the AlarmManager (and name of the thread)
# You may use alias : <repositoryid> and <repositoryname>
# fileprocessor.repository.JCMSUpload.postponed-alarmMgrName:

# 1. Schedule Period : duration in minutes between each check of postponed item in the repository
fileprocessor.repository.JCMSUpload.postponed-schedulePeriod: 5
# 2. Schedule Cron : 
#   for syntax usage details see http://community.jalios.com/jcms/jx_63894/jdring
fileprocessor.repository.JCMSUpload.postponed-scheduleCron:

