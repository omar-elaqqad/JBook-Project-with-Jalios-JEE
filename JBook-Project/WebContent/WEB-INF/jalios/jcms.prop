# --- Security ------------------------

# BCrypt hashing algorithm configuration
#  for Member passwords and cookies
#    - log2rounds set to 10, to limit CPU impact (was configured at 12 in JPlatform 10 SP4/SP5/SP6)
#    - require minimum of 10, as secured lowest 
#    - require maximum of 12, as explicitely specified with JPlatform 10 SP4/SP5/SP6   
channel.bcrypt.log2rounds: 10
channel.bcrypt.log2rounds.min: 10
channel.bcrypt.log2rounds.max: 12
#  for AuthKey 
auth-mgr.authkey-bcrypt-log2rounds: 4 

# Enable CSRF prevention
channel.security.csrf.enabled: true
# Authorize CSRF token validation against a token stored in member, and not
# only in current session.
# If enabled, security is decreased as it allows more time for
# an attacker to retrieve the token, but usability increase as the end user
# is allowed to submit a form after session expiration. 
channel.security.csrf.allow-token-through-session: true
# Duration in milliseconds above which a member token is considered invalid.
# Only applies to member token if allow-token-through-session is set to true.
# Default to one day
channel.security.csrf.member-token-max-age: 86400000
# Trust all Ajax requests, 
# ie request with HTTP Header "X-Requested-With" with value "XMLHttpRequest"
channel.security.csrf.trust-ajax-requests: true
# Trust all request performed by OpenAPI 
# ie request with HTTP Header "X-Jcms-CSRF-Header" set to any value 
channel.security.csrf.trust-openapi-requests: true

# Regex used for Validation of commonly received values 
channel.security.Validator.HTTPCookieName: ^[a-zA-Z0-9\\-_]{1,32}$
channel.security.Validator.HTTPCookieValue: ^[a-zA-Z0-9\\-\\/+=_ \\$:\\.]*$
channel.security.Validator.HTTPHeaderName: ^[a-zA-Z0-9\\-_]{1,32}$
channel.security.Validator.HTTPHeaderValue: ^[a-zA-Z0-9()\\-=\\*\\.\\?;,+\\/:&_ \\$\\"]*$
channel.security.Validator.JCMSDisplayServletPath: ^\\/?[a-zA-Z0-9.\\-\\/_=]*$
channel.security.Validator.JCMSChooserTarget: ^[a-zA-Z0-9.\\[\\]_]*$
# forbid URL containing \r \n, this will prevent HTTPSplitting attack
# forbid URL starting with /\ to prevent open redirection attack SEC-35
# forbid URL starting with "http:\\", "http:\/" or "http:/\" (and https variation) to prevent open redirection attack SEC-147
# forbid URL starting with "javascript:" to prevent XSS through Javascript URL redirection injection SEC-193
channel.security.Validator.JCMSRedirectURL=^(?!(\\/\\\\)|javascript:|(https?:((\\\\\\\\)|(\\/\\\\)|(\\\\\\/))))[^\\n\\r]*$
# Jalios Media (path or URL accepted)
channel.security.Validator.JCMSMediaPath=^(?!\\/\\\\)[^"'<>\\n\\r]*$
# Cookie configuration
channel.security.ForceHttpOnlyCookies: true
channel.security.ForceSecureCookies: false
channel.security.SameSiteCookies: Lax

# Validate redirect before using them
channel.security.validate-redirect: true 
# Authorized redirect pattern (regexp)  
channel.security.authorized-redirect.jalios: ^(https?:)?//[a-z]{2,15}\.jalios\.com(/[^\\s]*)?$
 
# Validation of "jsp" parameter through whitelist (added to default control of classic blacklist approach)
# Enable whitelist verification ? 
channel.security.authorized-jsp-whitelist: false
# Whitelisted JSP
channel.security.authorized-jsp.login: front/login.jsp
channel.security.authorized-jsp.signup: front/signup.jsp
channel.security.authorized-jsp.signupv: front/signupv.jsp
channel.security.authorized-jsp.mailpassword: admin/mail/mailPassword.jsp
channel.security.authorized-jsp.search: front/search.jsp
channel.security.authorized-jsp.query: front/query.jsp
channel.security.authorized-jsp.refine: front/refine.jsp
channel.security.authorized-jsp.caddyprint: work/caddy/printCaddy.jsp

# Validation of "jsp" parameter through blacklist (added to default control of classic blacklist approach)
# Enable blacklist verification ? 
channel.security.forbidden-jsp-blacklist: true
channel.security.forbidden-jsp.jcore-modal: jcore/modal/.*
channel.security.forbidden-jsp.jcore-tooltip: jcore/tooltip/.*
channel.security.forbidden-jsp.jcore-portal: jcore/portal/.*

# Validation of PortletJsp and PortletJspCollection "jsp" field
#  - require file ending in .jsp
#  - refuse any path contain ./  .\  ../  or  ..\
channel.security.portlet-jsp.authorized-jsp: ^(?!.*\\.\\.?[\\\\/])(.+\\.jsp)$

