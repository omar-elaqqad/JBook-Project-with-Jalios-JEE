# If true, '*' is appendend to words of a full text search in 'any' and 'all' mode
query.lucene.use-right-truncation: true
# A regular expression used to match words in user's search string for which right truncation will NOT be applied.
query.lucene.right-truncation.exclusion-regex: ^(.*[\u2200-\uFFFF].*)|(.*[\\p{Punct}&&[^&]])|(\\d+[€\\$]?)$

# Maximum number of boolean query in a lucene query (fix JCMS-5256)
query.lucene.max-clause-count: 8192

# Maximum number of result per search
query.lucene.mbr.max-results: 2000
query.lucene.cat.max-results: 100
query.lucene.pub.max-results: 1000
query.lucene.archive.max-results: 100

# Default fields in which member text search is performed (since JCMS-8122) 
query.lucene.mbr.searched-fields: lastname firstname organization locality region _allfields_

# Lucene fine tuning when writing in index
# integer value used to configure the maxBufferedDocs
# lucene.indexing.writer.max-buffered-docs: 
# integer value used to configure the ramBufferSize in MB
# lucene.indexing.writer.ram-buffer-size-mb: 
# double value used to configure the maxMergeCount of the ConcurrentMergeScheduler
# lucene.indexing.cms.max-merge-count: 
# double value used to configure the maxThreadCount of the ConcurrentMergeScheduler
# lucene.indexing.cms.max-thread-count: 

# Publication indexing configuration : 
# Should we index each publication field in a dedicated lucene field ? (available since JCMS-5296)
#
#  if set to false (recommended default) :
#    only the fields required for default JCMS search behavior will be provided : 
#     - "title" : for publications' title
#     - "abstract" : for publications' abstract
#     - "fdContent" : for file document content (used if searchInFiles=true)
#     - "fdContentType" : for document kind search (using documentKinds=...)
#     - and other required technical fields : 
#         "_idxdate_", "authorId", "pstatus", "workspaceId", "version",
#         "fdIdxDate", "fdFilename", "fdOriginalFilename", "fdModified"
#    this is the recommended configuration to reduce lucene publication index size and improve indexing and search performance
#
#  if set to true (behavior applied before introduction of this property) : 
#    ALL publication fields will have a dedicated lucene field (completing the fields already index when configuration is set to false)
#    this configuration allows advanced search features to be implemented
#      * per field search using either 
#         - the QueryHandler "searchedFields" option (eg searchedFields=myfield&text=foo )
#         - or the advanced lucene search syntax (eg mode=advanced&text=myfield:foo )
#      * per field boost at indexing and search using a custom LuceneSearchEnginePolicyFilter
#   
lucene.pub-indexing.map-all-pub-fields-to-lucene-fields: false


# Number of words above which a suggestion is NOT proposed, set to 0 to disable spell suggest
spellsuggest.words-threshold: 2

# Optimize indices Every monday at 2h30 
search-engine.optimize-schedule: 30 2 * * 2 *

# Automatically reindex all data on first startup when lucene index does not yet exists
search-engine.auto-indexing-on-first-startup: true

# Automatically index, on startup, any created or modified Data since last index update (JCMS-8170)
# (if all directory of this search engine exists and have the information of their last modification date available) 
search-engine.auto-indexing-data-modified: true

