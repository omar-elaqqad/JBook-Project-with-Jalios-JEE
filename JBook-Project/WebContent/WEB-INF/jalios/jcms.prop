# Audience Rights
audience-rigths.enabled: false
audience-rigths.axes: 

# Reverse Proxy
channel.reverse-proxy.enabled: false
channel.reverse-proxy.conf1.ip-addresses: 192.168.0.1, 192.168.0.2
