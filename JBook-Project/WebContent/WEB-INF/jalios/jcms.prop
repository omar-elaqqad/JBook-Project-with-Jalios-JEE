# ----------------------------------------------------------------------
# Trash
# ----------------------------------------------------------------------

# Is automatic delete after a specific duration is enabled or not
jcms.trash.automatic-delete.enabled: true
# duration in days, publications are kept in trash before being permanently deleted automatically.
jcms.trash.automatic-delete.delay: 15
# trashable types : types of publication which can be put in the trash
jcms.trash.trashable-types.types: Content UserContent
# untrashable types : types of publication which cannot be put in the trash (since JCMS-9314)
jcms.trash.untrashable-types.types:
# Propose final delete (to end user) for trashable type (i.e. types which can be put in trash).
# - If true, final delete action will be displayed to all users.
# - If false, final delete action will only be displayed to administrators.
jcms.trash.final-delete.enabled: false

