# ------------------------------------------------------------
# Portlet overrides
# ------------------------------------------------------------
jcmsplugin.collaborativespace.override-cache.enabled: true
jcmsplugin.collaborativespace.overridable-portlet.PortletCarousel: autoplaycarousel slidesToShow dots numberOfLinesInTheTitle
jcmsplugin.collaborativespace.overridable-portlet.PortletImage: image alt link imgWidth imgHeight
jcmsplugin.collaborativespace.overridable-portlet.PortletIndicator:    value
jcmsplugin.collaborativespace.overridable-portlet.PortletQueryForeach:    orderBy maxResults pageSizes 
jcmsplugin.collaborativespace.overridable-portlet.PortletQueryForeachDetail:    orderBy maxResults pageSizes 
jcmsplugin.collaborativespace.overridable-portlet.PortletQueryForeachCards:    orderBy maxResults pageSizes  cardsPerLine
jcmsplugin.collaborativespace.overridable-portlet.PortletRSS:    source maxItems
jcmsplugin.collaborativespace.overridable-portlet.PortletWorkflow:    pageSizes orderBy
jcmsplugin.collaborativespace.overridable-portlet.PortletWYSIWYG:    wysiwyg
jcmsplugin.collaborativespace.overridable-portlet.PortletPublication:    pub template showDisplayHeader showDisplayFooter

jportal.sidebar.ui-customizable-template.collaborativeSpace.borderless: true
jportal.sidebar.ui-customizable-template.collaborativeSpace.default: true
jportal.sidebar.ui-customizable-template.collaborativeSpace.noSkin: true
jportal.sidebar.ui-customizable-template.collaborativeSpace.lightSkin: true

