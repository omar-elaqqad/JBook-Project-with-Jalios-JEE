<li role="tab" title="<%= tab.getLabel(userLang) %>" <%= handler.isActiveTab(tab) ? "class=\"active\" aria-current=\"true\"" : "" %>><a id="site-tab-<%= tab.getName() %>" tabindex="0" data-jalios-action="ajax-refresh" data-jalios-options='{ "focusElement": ".sidebar-tab-title", "noscroll":true,"nohistory":true, "params" : { "sidebarTab" : "<%= tab.getName() %>" }}'><jalios:icon src="<%= tab.getIcon() %>" alt="<%= tab.getIconAlt(userLang) %>" /></a></li>