    &lt;td width="&lt;%= (itLevel.intValue() + 1) * 20 %&gt;"&gt;&amp;nbsp;&lt;/td&gt;
    &lt;td width="&lt;%= 500 - ((itLevel.intValue() + 1) * 20) %&gt;"&gt;
